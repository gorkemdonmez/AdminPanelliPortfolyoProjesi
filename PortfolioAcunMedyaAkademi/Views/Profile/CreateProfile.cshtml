@model PortfolioAcunMedyaAkademi.Models.TblProfile
@{
    ViewBag.Title = "CreateProfile";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Profil Ekleme</h2>
<form method="post">

    <label>ID</label>
    @Html.TextBoxFor(x => x.ProfileId, new { @class ="form-control"})
    <br />
    <label>İsmi</label>
    @Html.TextBoxFor(x => x.Name, new { @class = "form-control" })
    <br />
    <label>Doğum Tarihi</label>
    @Html.TextBoxFor(x => x.Birthday, new { @class = "form-control" })
    <br />
    <label>Adresi</label>
    @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
    <br />
    <label>Emaili</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Telefon Numarası</label>
    @Html.TextBoxFor(x => x.Telephone, new { @class = "form-control" })
    <br />

    <button class="btn btn-info">Profil Kaydet</button>


</form>
